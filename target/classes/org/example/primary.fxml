            <TableColumn fx:id="empresaCol" minWidth="50.0" prefWidth="100.0" text="Empresa" />
         </columns>
      </TableView>
      <HBox alignment="BOTTOM_RIGHT" fillHeight="false" maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="200.0">
